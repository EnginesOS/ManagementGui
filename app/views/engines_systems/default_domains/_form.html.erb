<%= custom_form_for @default_domain,
    url: system_default_domain_path(engines_system_id: @engines_system.id),
    method: :patch do |f| %>
<%= f.engines_field :domain, as: :select,
    collection: @engines_system.domains.map{|domain| [domain[:domain_name]]} %>
<%= f.engines_cancel_submit url: system_domains_path(engines_system_id: @engines_system.id), remote: true %>
<% end %>
